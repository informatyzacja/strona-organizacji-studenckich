    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.45.0",
    "@trpc/next": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "framer-motion": "^11.0.3",
    "fuse.js": "^7.0.0",
    "jotai": "^2.6.4",
    "next": "14.1.0",
    "nextjs-routes": "^2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.0.1",
    "react-image-gallery": "^1.3.0",
