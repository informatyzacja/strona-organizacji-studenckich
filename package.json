    "@typescript-eslint/parser": "^6.19.1",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "graphql-zeus": "^5.3.3",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "prettier": "^3.1.1",
    "semantic-release": "^23.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "wait-on": "^7.2.0"
